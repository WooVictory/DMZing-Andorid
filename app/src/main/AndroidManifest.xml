        </activity>
        <activity android:name=".view.MainActivity"></activity>
        <activity
            android:name=".view.login.LoginActivity"
            android:windowSoftInputMode="adjustResize"></activity>
        <activity android:name=".view.signup.SignUpOneActivity"></activity>
        <activity android:name=".view.signup.SignUpTwoActivity"></activity>
        <activity android:name=".view.signup.SignUpThreeActivity"></activity>
        <activity android:name=".view.mypage.point.MypagePointActivity"></activity>
        <activity android:name=".view.mypage.MypageReviewActivity"></activity>
        <activity android:name=".view.mypage.MyLetterActivity"></activity>
        <activity android:name=".view.mypage.setting.SettingActivity"></activity>
        <activity android:name=".view.mypage.setting.ServiceActivity"></activity>
        <activity android:name=".view.mypage.setting.ServiceDetailActivity"></activity>
        <activity android:name=".view.review.ReviewListActivity"></activity>
        <activity android:name=".view.course.CourseDetailActivity"></activity>
        <activity
            android:name=".view.review.PhotoDetailActivity"
            android:theme="@style/Theme.AppCompat.ActivityTransparent"></activity>
        <activity android:name=".view.mypage.MypageCourseActivity">
        </activity>
    </application>

</manifest>